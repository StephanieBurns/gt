

<h1>Listing Tracks</h1>

<table>
  <thead>
    <tr>
      <th>Success 1</th>
      <th>Success 2</th>
      <th>Success 3</th>
      <th>Goal 1</th>
      <th>Goal 2</th>
      <th>Goal 3</th>
      <th>Goal 4</th>
      <th>Goal 5</th>
      <th>Goal 6</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tracks.each do |track| %>
      <tr>
        <td><%= track.s1 %></td>
        <td><%= track.s2 %></td>
        <td><%= track.s3 %></td>
        <td><%= track.g1 %></td>
        <td><%= track.g2 %></td>
        <td><%= track.g3 %></td>
        <td><%= track.g4 %></td>
        <td><%= track.g5 %></td>
        <td><%= track.g6 %></td>
        <td><%= link_to 'Show', track %></td>
        <td><%= link_to 'Edit', edit_track_path(track) %></td>
        <td><%= link_to 'Destroy', track, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Track', new_track_path %>
